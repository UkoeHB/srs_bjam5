{
"#import": {
    "constants.caf.json": ""
},

"#commands": {
    "LoadImages": [[
        {"image": "$background_tile_texture"},
        {"image": "$boundary_side_texture"},
        {"image": "$boundary_corner_texture"},
        {"image": "$controls_texture"},
        {"image": "$collectable_exp_texture"},
        {"image": "$collectable_karma_texture"},
        {"image": "$collectable_healthpack_texture"}
    ]],
    "LoadSpriteAnimations": [[
        {
            "name": "$player_standing_animation",
            "image": "$player_standing_texture",
            "size": "$player_sprite_size",
            "columns": 2,
            "rows": 1,
            "frame_time": 300,
            "clips": [{"frames": {"Row": 0}}]
        }
    ]],
    "LoadAudio": [[
        {"audio": "$loop1"}
    ]],
    "SoundtrackDatabase": {
        "tracks": [
            {"source": "$loop1", "volume": 0.3}
        ]
    }
}
}
