{
"#import": {
    "constants.caf.json": ""
},

"scene": {
    "FlexStyle": {
        "dims": {"width": {"Vw": 100.0}, "height": {"Vh": 100.0}},
        "content": {
            "flex_direction": "Row",
            "justify_main": "SpaceEvenly",
            "justify_cross": "Center",
            "padding": {"left": {"Percent": 15.0}, "right": {"Percent": 15.0}}
        }
    },
    "BgColor": [{"Hsla": {"hue": 0.0, "saturation": 0.80, "lightness": 0.0, "alpha": 0.5}}]
},

"filler_notification": {
    "AbsoluteStyle": {
        "dims": {"top": "Auto", "left": "Auto"},
        "content": {
            "padding": {"top": {"Px": 400.0}}
        }
    },
    "text": {
        "TextLine": {"text": "All power-ups are max level."}
    }
},

"powerup_frame_scene": {
    "FlexStyle": {
        "dims": {
            "width": {"Px": 250.0},
            "height": {"Px": 300.0}
        },
        "content": {
            "flex_direction": "Column",
            "justify_main": "SpaceEvenly",
            "justify_cross": "Center",
            "padding": {"top": {"Px": 50.0}, "bottom": {"Px": 50.0}, "left": {"Px": 15.0}, "right": {"Px": 15.0}}
        }
    },
    "Splat<Border>": [{"Px": 4.0}],
    "BgColor": [{"Hsla": {"hue": 120.0, "saturation": 0.7, "lightness": 0.28, "alpha": 1.0}}],
    "BrColor": [{"Hsla": {"hue": 120.0, "saturation": 0.85, "lightness": 0.10, "alpha": 1.0}}],
    "BrRadius": [{"Px": 30.0}],
    "Interactive": []
},

"powerup_scene": {
    "FlexStyle": {
        "dims": {
            "width": {"Percent": 100.0}
        },
        "content": {
            "flex_direction": "Column",
            "justify_main": "Center",
            "justify_cross": "Center"
        }
    },

    "icon": {
        "FlexStyle": {
            "dims": {
                "max_width": {"Percent": 100.0},
                "max_height": {"Px": 100.0},
                "width": {"Px": 100.0}
            }
        }
    },

    "title": {
        "FlexStyle": {
            "flex": {
                "margin": {"top": {"Px": 20.0}, "bottom": {"Px": 25.0}}
            }
        },
        "TextLine": {}
    },

    "description": {
        "TextLine": {"size": 22.0, "justify": "Center", "linebreak": "WordBoundary"}
    }
},

"filler_scene": {
    "TextLine": {"justify": "Center", "linebreak": "WordBoundary"}
}
}
